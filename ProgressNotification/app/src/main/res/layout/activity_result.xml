<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.akexorcist.progressnotification.com.akexorcist.progressnotification.ui.ResultActivity">

    <TextView
        android:id="@+id/tv_user_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/text_view_user_id_margin_bottom"
        android:layout_marginTop="@dimen/default_margin_large"
        android:textSize="@dimen/text_size_large"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="User ID" />

    <TextView
        android:id="@+id/tv_account_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/default_margin_small"
        android:textSize="@dimen/text_size"
        app:layout_constraintEnd_toEndOf="@+id/tv_user_id"
        app:layout_constraintStart_toStartOf="@+id/tv_user_id"
        app:layout_constraintTop_toBottomOf="@+id/tv_user_id"
        tools:text="TextView" />
</android.support.constraint.ConstraintLayout>
