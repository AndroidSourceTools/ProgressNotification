<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ResultActivity2">

    <TextView
        android:id="@+id/tvUserId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/default_margin_large"
        android:layout_marginBottom="@dimen/text_view_user_id_margin_bottom"
        android:textSize="@dimen/text_size_large"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="User ID" />

    <TextView
        android:id="@+id/tvAccountStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/default_margin_small"
        android:textSize="@dimen/text_size"
        app:layout_constraintEnd_toEndOf="@+id/tvUserId"
        app:layout_constraintStart_toStartOf="@+id/tvUserId"
        app:layout_constraintTop_toBottomOf="@+id/tvUserId"
        tools:text="TextView" />
</androidx.constraintlayout.widget.ConstraintLayout>
